{
  "name": "spiral/storage",
  "type": "library",
  "description": "Storage API: Amazon, Rackspace, FTP, SFTP, Filesystem, GridFS",
  "license": "MIT",
  "authors": [
    {
      "name": "Anton Titov / Wolfy-J",
      "email": "wolfy.jd@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "spiral/common": "^0.9",
    "psr/simple-cache": "^1.0",
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "mockery/mockery": "^0.9.4",
    "vlucas/phpdotenv": "^2.1",
    "mongodb/mongodb": "^1.0.1"
  },
  "suggest": {
    "mongodb/mongodb": "To enable support of GridFS storage"
  },
  "autoload": {
    "psr-4": {
      "Spiral\\": "source/Spiral/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Spiral\\Tests\\": "tests/"
    }
  }
}
